	"eighth",
	"nineth",
	"tenth"
];
this.ColorPalette.unionPalettes <- function ( sourcePalette, targetPalette )
{
	if (typeof targetPalette == "string")
	{
		targetPalette = [
			targetPalette
		];
	}

	targetPalette = this.deepClone(targetPalette);

	foreach( i, x in sourcePalette )
	{
		local found = false;

		foreach( j, y in targetPalette )
		{
			if (y == x)
			{
				found = true;
			}
		}

		if (!found)
		{
			targetPalette.append(x);
		}
	}

	return targetPalette;
};
this.ColorPalette.arrayToTable <- function ( a )
{
	if (this.type(a) != "array")
	{
		throw "ColorPalette::arrayToTable - a was passed something other than an array";
	}

	this.t <- {};

	for( local i = 0; i < a.len(); i++ )
	{
		if (a[i] != null && a[i] != "")
		{
			this.t[this.ColorSlotNames[i]] <- a[i];
		}
	}

	return this.t;
};
this.ColorPalette.tableToArray <- function ( t )
{
	if (this.type(t) != "table")
	{
		throw "ColorPalette::tableToArray - t was passed something other than a table";
	}

	t = this.deepClone(t);
	this.a <- [];
	local total = t.len();

	for( local i = 0; i < total; i++ )
	{
		if (t.len() > 0)
		{
			if (this.ColorSlotNames[i] in t)
			{
				this.a.append(t[this.ColorSlotNames[i]]);
				delete t[this.ColorSlotNames[i]];
			}
			else
			{
				this.a.append(null);
				total++;
			}
		}
	}

	return this.a;
};
